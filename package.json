  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:ts": "node -r tsconfig-paths/register -r ts-node/register/transpile-only ./src/migrate.ts",
    "migrate:js": "node ./dist/migrate.js",
    "tsc:check": "tsc --project tsconfig.json --noEmit",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=50%",
    "test:cov": "jest --maxWorkers=50% --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.config.ts --runInBand",
    "test:e2e:watch": "jest --config ./test/jest-e2e.config.ts --watch --runInBand",
    "rabbitmq:consumers:dev": "npm run start:dev -- --entryFile cmd/rabbitmq",
    "rabbitmq:consumers:prod": "node dist/cmd/rabbitmq"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^5.6.1",
    "@google-cloud/storage": "7.13.0",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/event-emitter": "2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/sequelize": "^10.0.1",
    "chance": "1.1.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "16.4.5",
    "eventemitter2": "6.4.9",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "mysql2": "^3.11.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sequelize": "6.37.3",
    "sequelize-typescript": "2.1.6",
    "sqlite3": "5.1.7",
    "umzug": "^3.8.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.4",
